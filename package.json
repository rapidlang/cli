{
  "name": "@rapid/cli",
  "version": "0.1.3",
  "main": "bin/index.js",
  "scripts": {
    "start": "serve build & nodemon -e rapid --watch ./src --exec 'rapid src/main.rapid -o build/main.wasm'",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "repository": "https://github.com/rapidlang/cli.git",
  "author": "Michal Jach <michaljach@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "rapid-lang": "^0.9.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "git-cz": "^4.3.1",
    "husky": "^4.2.3",
    "nodemon": "^2.0.2",
    "standard-version": "^7.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
